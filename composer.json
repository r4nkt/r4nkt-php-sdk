{
    "name": "r4nkt/r4nkt-php-sdk",
    "description": "An SDK to easily work with the r4nkt API",
    "keywords": [
        "r4nkt",
        "gamification",
        "leaderboard",
        "achievements",
        "api",
        "sdk"
    ],
    "homepage": "https://github.com/r4nkt/r4nkt-php-sdk",
    "license": "MIT",
    "authors": [
        {
            "name": "Travis Elkins",
            "email": "travis.elkins+github@gmail.com",
            "homepage": "https://r4nkt.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "guzzlehttp/guzzle": "^7.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3",
        "vimeo/psalm": "^4.4"
    },
    "autoload": {
        "psr-4": {
            "R4nkt\\PhpSdk\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "R4nkt\\PhpSdk\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "./vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
