{
    "name": "r4nkt/r4nkt-php-sdk",
    "description": "An SDK to easily work with the r4nkt API",
    "keywords": [
        "r4nkt",
        "gamification",
        "leaderboard",
        "achievements",
        "api",
        "sdk"
    ],
    "homepage": "https://github.com/r4nkt/r4nkt-php-sdk",
    "license": "MIT",
    "authors": [
        {
            "name": "Travis Elkins",
            "email": "travis.elkins+github@gmail.com",
            "homepage": "https://r4nkt.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4",
        "guzzlehttp/guzzle": "^6.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "R4nkt\\PhpSdk\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "R4nkt\\PhpSdk\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    }
}
